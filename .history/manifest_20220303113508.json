{

    "name": "Wordle Solver",
    
    "description": "Get Wordle Solution on NYTimes in seconds",
    
    "version": "1.0",
    
    "manifest_version": 3,

    "permissions": ["storage","activeTab"],

    "background": {

        "service_worker": "background.js"
        
        },

    "action": {

        "default_popup": "popup.html"
        
        },

    "content_scripts": [
            {
              "js": ["content-script.js"]
            }
    ]
    
    }